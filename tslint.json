{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-prettier",
        "tslint-plugin-prettier"
    ],
    "jsRules": {
        "no-console": false,
        "object-literal-sort-keys": false,
        "prettier": true
    },
    "rules": {
        "interface-name": false,
        "no-console": false,
        "no-var-requires": false,
        "object-literal-sort-keys": false,
        "prettier": true
    },
    "rulesDirectory": []
}